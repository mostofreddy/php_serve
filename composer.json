{
    "name": "smarttly/php-serve",
    "type": "library",
    "description": "PHP Built-in web server",
    "keywords": ["rest", "api", "framework", "command", "server", "development", "built-in"],
    "homepage": "https://github.com/mostofreddy/php_serve",
    "license": "MIT",
    "version": "0.1.0",
    "authors": [
        {
            "name": "Federico Lozada Mosto",
            "email": "mosto.federico@gmail.com",
            "homepage": "https://github.com/mostofreddy"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "symfony/console": "5.0.*",
        "symfony/process": "5.0.*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "Smarttly\\Phpserve\\": "src/"
        }
    },
    "bin": [
        "php-serve"
    ],
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
